  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for SwiftPractic

  target 'SwiftPracticTests' do
    inherit! :search_paths
    
    # Pods for testing
  end

  target 'SwiftPracticUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
